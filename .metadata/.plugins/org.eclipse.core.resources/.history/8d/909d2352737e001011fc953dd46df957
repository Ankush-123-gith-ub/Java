package myfirstpackage;
class CAR{
	
	String model;
	int year;
	
	// method to display
	void display() {
		System.out.println("Model = "+ model + ", Year = " + year);
	}
}
public class car {
	public static void main(String args[]){
		
		// first object
		CAR car1 = new CAR();
		car1.model = "Toyota";
		car1.year = 1999;
		
		//second object
		CAR car2 = new CAR();
		car2.model = "Supra";
		car2.year = 1997;
		
		// to call out function
		car1.display();
		car2.display();
		
	}
	
}
