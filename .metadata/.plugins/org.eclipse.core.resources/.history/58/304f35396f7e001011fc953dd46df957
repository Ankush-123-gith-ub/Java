package myfirstpackage;

import java.util.Scanner;

public class q1 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if (a % 2 == 0 || a % 3 == 0) {
			System.out.println("Yes! It is divisible");
		}
		else {
			System.out.println("Not divisible");
		}
		
	}
}
